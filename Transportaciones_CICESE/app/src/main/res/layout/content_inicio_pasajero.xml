<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fff"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.transporte.cicese.transportaciones_cicese.InicioPasajeroActivity"
    tools:showIn="@layout/app_bar_inicio_pasajero"
    android:id="@+id/ContenedorMain">

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Bienvenido"
        android:textSize="30sp"
        android:layout_marginTop="100dp"
        tools:ignore="HardcodedText"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="150dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/logocicese"
        android:layout_below="@+id/textView6"
        android:layout_alignStart="@+id/textView6"
        android:layout_marginTop="25dp"
        tools:ignore="ContentDescription" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageView2"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="8dp"
        android:text="Transportaciones"
        android:textSize="18sp"
        tools:ignore="HardcodedText" />

    <!--Button
        android:id="@+id/button2"
        android:layout_width="160dp"
        android:layout_height="35dp"
        android:background="@color/colorPrimaryText"
        android:text="@string/transportaciones"
        android:textAllCaps="false"
        android:textColor="@android:color/background_light"
        android:textSize="20sp"
        android:layout_below="@+id/imageView2"
        android:layout_centerHorizontal="true" /-->

</RelativeLayout>
